<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bureau.rclaplaine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenActivity"
            android:label="RClaPlaine"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity
            android:name=".WomenActivity"
            android:label="RClaPlaine"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity
            android:name=".JuniorActivity"
            android:label="RClaPlaine"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity
            android:name=".DetailJuniorActivity"
            android:label="RClaPlaine"
            android:parentActivityName=".JuniorActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".JuniorActivity"/>
        </activity>
        <activity
            android:name=".DetailMenActivity"
            android:label="RClaPlaine"
            android:parentActivityName=".MenActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MenActivity"/>
        </activity>
        <activity
            android:name=".DetailWomenActivity"
            android:label="RClaPlaine"
            android:parentActivityName=".WomenActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WomenActivity"/>
        </activity>
        <activity
            android:name=".OrderActivity"
            android:label="RClaPlaine"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
    </application>

</manifest>